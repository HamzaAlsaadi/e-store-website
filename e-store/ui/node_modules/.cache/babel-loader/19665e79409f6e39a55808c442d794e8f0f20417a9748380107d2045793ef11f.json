        id: id,\n        name: name_mobile,\n        Price: Price,\n        img: img,\n        count: 1\n      };\n      store.state.counter++;\n      console.log(store.state.Order);\n    }\n  },\n  beforeMount() {\n    this.getorders();\n    this.getoffer();\n  }\n};","map":{"version":3,"names":["axios","store","FooTer","HeaderAllCategories","name","components","data","img","product","idoffer","counter","methods","getoffer","method","url","then","response","console","log","catch","error","window","alert","getorders","state","products","index","AddProduct","id","name_mobile","Price","Object","keys","Order","length","count","beforeMount"],"sources":["C:\\Users\\ASUSD\\Documents\\GitHub\\e-store-website\\e-store\\ui\\src\\views\\OfferProduct.vue"],"sourcesContent":["<template>\r\n    <!-- Header All Categories-->\r\n    <HeaderAllCategories />\r\n    <section class=\"section-products\">\r\n        <div class=\"container\">\r\n            <div class=\"row justify-content-center text-center\">\r\n                <div class=\"col-md-8 col-lg-6\">\r\n                    <div class=\"header\">\r\n                        <h2>Offers</h2>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"row\">\r\n                <div\r\n                    class=\"col-md-6 col-lg-4 col-xl-3\"\r\n                    v-for=\"product in $store.state.products\"\r\n                    :key=\"product.id\"\r\n                    id=\"product-1\"\r\n                >\r\n                    <div\r\n                        v-if=\"product.offer_id != 2\"\r\n                        id=\"product-2\"\r\n                        class=\"single-product\"\r\n                    >\r\n                        <div\r\n                            class=\"part-1\"\r\n                            :style=\"{\r\n                                backgroundImage: 'url(' + product.imge + ')',\r\n                            }\"\r\n                        >\r\n         
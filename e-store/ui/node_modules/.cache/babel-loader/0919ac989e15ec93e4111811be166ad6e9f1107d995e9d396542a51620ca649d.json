rage.getItem(\"token\");\r\n\r\n            axios({\r\n                method: \"get\",\r\n                url: \"http://127.0.0.1:8000/api/user/all-user\",\r\n\r\n                headers: {\r\n                    Authorization: `Bearer ${token}`,\r\n                    \"Content-Type\": \"multipart/form-data\",\r\n                },\r\n            })\r\n                .then((response) => {\r\n                    console.log(response);\r\n                    this.users = response.data;\r\n                })\r\n                .catch(function (error) {\r\n                    console.log(error.data);\r\n                    console.log(error.response);\r\n                })\r\n                .catch(function () {\r\n                    window.alert(\"hi\");\r\n                });\r\n        },\r\n    },\r\n    mounted() {\r\n        this.getusers();\r\n    },\r\n};\r\n</script>\r\n<style scoped>\r\n* {\r\n    margin: 0;\r\n    padding: 0;\r\n    box-sizing: border-box;\r\n    font-family: \"poppins\", \"sans-serif\";\r\n}\r\n.table {\r\n    border-collapse: collapse;\r\n    table-layout: fixed;\r\n    font-size: 15px;\r\n    min-width: 100%;\r\n    overflow: hidden;\r\n    border-radius: 4px;\r\n}\r\n\r\n.table thead tr {\r\n    background: #000000;\r\n    text-align: left;\r\n    font-weight: bold;\r\n    color: white;\r\n}\r\n\r\n.table th {\r\n    padding: 12px 15px;\r\n    text-align: center;\r\n    word-break: break-all;\r\n    color: white;\r\n}\r\ntr td a {\r\n    color: #000000;\r\n}\r\n.dd {\r\n    font-size: 30px;\r\n\r\n    color: #000000;\r\n}\r\n\r\n.dd:hover {\r\n    color: #3f3e3e;\r\n}\r\n.table td {\r\n    padding: 12px 15px;\r\n    text-align: center;\r\n    word-break: break-all;\r\n}\r\n\r\n.table tbody tr {\r\n    border-bottom: 1px solid #ddd;\r\n    background: #e9e9e9;\r\n}\r\n\r\n.table tbody tr:last-of-type {\r\n    border-bottom: 2px solid #ffffff;\r\n}\r\n\r\n.table button {\r\n    padding: 6px 20px;\r\n    border-radius: 10px;\r\n    curso
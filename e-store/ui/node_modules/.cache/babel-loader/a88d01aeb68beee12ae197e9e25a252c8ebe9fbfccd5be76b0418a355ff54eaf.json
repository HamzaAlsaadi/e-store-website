);\n  }\n};","map":{"version":3,"names":["HeaderAdmin","FooTer","axios","name","components","data","isPopupVisible","namecategory","idcategory","categorys","methods","editData","cancelEdit","saveEdit","console","log","Addcategory","method","url","Company_id","then","response","alert","catch","error","window","getcategory","deletecategory","id","location","reload","mounted"],"sources":["C:\\Users\\ASUSD\\Documents\\GitHub\\e-store-website\\e-store\\ui\\src\\views\\AddCategory.vue"],"sourcesContent":["<template>\r\n    <body>\r\n        <HeaderAdmin />\r\n        <div class=\"container\">\r\n            <header>Add Category</header>\r\n\r\n            <form action=\"#\">\r\n                <div class=\"form\">\r\n                    <div class=\"detalis\">\r\n                        <span class=\"title\">Detalis </span>\r\n\r\n                        <div class=\"fields\">\r\n                            <div class=\"input-field\">\r\n                                <label> Name Category </label>\r\n                                <input\r\n                                    v-model=\"namecategory\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Company ID </label>\r\n                                <input\r\n                                    v-model=\"idcategory\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <button\r\n                            type=\"button\"\r\n                            class=\"nextbtn\"\r\n                            @click=\"Addcategory()\"\r\n                        >\r\n       
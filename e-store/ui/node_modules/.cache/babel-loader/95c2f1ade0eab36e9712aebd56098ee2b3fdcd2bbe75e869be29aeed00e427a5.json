{"ast":null,"code":"import axios from \"axios\";\nimport HeaderAdmin from \"@/components/HeaderAdmin.vue\";\nimport FooTer from \"@/components/footer.vue\";\nexport default {\n  name: \"AddProduct\",\n  components: {\n    HeaderAdmin,\n    FooTer\n  },\n  data() {\n    return {\n      isPopupVisible: false,\n      comid: \"\",\n      img: \"\",\n      catid: \"\",\n      price: \"\",\n      type: \"\",\n      screen: \"\",\n      back: \"\",\n      front: \"\",\n      batt: \"\",\n      gpu: \"\",\n      offerid: \"\",\n      mobilename: \"\",\n      cpu: \"\",\n      Products: [],\n      file: null,\n      idpro: null,\n      selectedFile: null,\n      csv: \"\"\n    };\n  },\n  methods: {\n    changecsv(event) {\n      this.csv = event.target.files[0];\n    },\n    changefile() {\n      const input = document.getElementById(\"image\");\n      const file = input.files;\n      this.img = file;\n      console.log(this.img);\n    },\n    editData(qw) {\n      this.isPopupVisible = true;\n      this.idpro = qw;\n    },\n    cancelEdit() {\n      this.isPopupVisible = false;\n    },\n    saveEdit() {\n      console.log(\"save ok\");\n      this.isPopupVisible = false;\n    },\n    submitFile() {\n      const formData = new FormData();\n      // get the item\n      // append data\n      formData.append(\"csv_file\", this.csv);\n      console.log(this.csv);\n      const token = window.localStorage.getItem(\"token\");\n      axios.post(\"http://127.0.0.1:8000/api/user/store/product/csv\", formData, {\n        headers: {\n          Authorization: `Bearer ${token}`,\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(response => {\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    deleteproduct(id) {\n      axios({\n        method: \"post\",\n        url: \" http://127.0.0.1:8000/api/delete-product?id=\" + id\n      }).then(response => {\n        console.log(response);\n        window.alert(\"OK\");\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    Addproduct() {\n      const formData = new FormData();\n      // get the item\n      const imgFile = document.querySelector(\"#image\");\n      // append data\n      formData.append(\"mobile_name\", this.mobilename);\n      formData.append(\"Cpu_spsecfication\", this.cpu);\n      formData.append(\"Gpu_spsecfication\", this.gpu);\n      formData.append(\"battery_spsecfication\", this.batt);\n      formData.append(\"Front_camera_spsecfication\", this.front);\n      formData.append(\"Back_camera_spsecfication\", this.back);\n      formData.append(\"Screen_Size\", this.screen);\n      formData.append(\"Type_of_charge\", this.type);\n      formData.append(\"Price\", this.price);\n      formData.append(\"imge\", imgFile.files[0]);\n      formData.append(\"category_id\", this.catid);\n      formData.append(\"Company_id\", this.comid);\n      formData.append(\"offer_id\", this.offerid);\n      console.log(formData);\n      axios.post(\"http://127.0.0.1:8000/api/create-product\", formData).then(response => {\n        console.log(response);\n        alert(response.data);\n      }).catch(function (error, response) {\n        console.log(error.data);\n        console.log(error.response);\n        console.log(response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    editproduct() {\n      axios({\n        method: \"post\",\n        url: \"http://127.0.0.1:8000/api/update-product?mobile_name=\" + this.mobilename + \"&Cpu_spsecfication=\" + this.cpu + \"&Gpu_spsecfication=\" + this.gpu + \"&battery_spsecfication=\" + this.batt + \"&Front_camera_spsecfication=\" + this.front + \"&Back_camera_spsecfication=\" + this.back + \"&Screen_Size=\" + this.screen + \"&Type_of_charge=\" + this.type + \"&Price=\" + this.price + \"&category_id=\" + this.catid + \"&Company_id=\" + this.comid + \"&id=\" + this.idpro,\n        data: {\n          Company_id: this.comid\n        }\n      }).then(response => {\n        console.log(response);\n        alert(response.data);\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    getproduct() {\n      axios({\n        method: \"get\",\n        url: \"http://127.0.0.1:8000/api/products/\"\n      }).then(response => {\n        console.log(response);\n        this.Products = response.data;\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    }\n  },\n  mounted() {\n    this.getproduct();\n  }\n};","map":{"version":3,"names":["axios","HeaderAdmin","FooTer","name","components","data","isPopupVisible","comid","img","catid","price","type","screen","back","front","batt","gpu","offerid","mobilename","cpu","Products","file","idpro","selectedFile","csv","methods","changecsv","event","target","files","changefile","input","document","getElementById","console","log","editData","qw","cancelEdit","saveEdit","submitFile","formData","FormData","append","token","window","localStorage","getItem","post","headers","Authorization","then","response","catch","error","deleteproduct","id","method","url","alert","Addproduct","imgFile","querySelector","editproduct","Company_id","getproduct","mounted"],"sources":["C:\\Users\\ASUSD\\Documents\\GitHub\\e-store-website\\e-store\\ui\\src\\views\\AddProduct.vue"],"sourcesContent":["<template>\r\n    <body>\r\n        <HeaderAdmin />\r\n        <div class=\"oo\">\r\n            <p class=\"order\">Mangment Product</p>\r\n            <div class=\"add-product\">\r\n                <button class=\"s1\" @click=\"submitFile\">Import</button>\r\n\r\n                <input\r\n                    type=\"file\"\r\n                    @change=\"changecsv\"\r\n                    accept=\"csv/*\"\r\n                    ref=\"file\"\r\n                    id=\"csv\"\r\n                    name=\"csv\"\r\n                />\r\n            </div>\r\n        </div>\r\n        <div class=\"container\">\r\n            <header>Add Product</header>\r\n\r\n            <form action=\"#\">\r\n                <div class=\"form\">\r\n                    <div class=\"detalis\">\r\n                        <span class=\"title\">Detalis </span>\r\n\r\n                        <div class=\"fields\">\r\n                            <div class=\"input-field\">\r\n                                <label>Mobile Name</label>\r\n                                <input\r\n                                    v-model=\"mobilename\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Cpu Spsecfication</label>\r\n                                <input\r\n                                    v-model=\"cpu\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Gpu Spsecfication</label>\r\n                                <input\r\n                                    v-model=\"gpu\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n\r\n                            <div class=\"input-field\">\r\n                                <label>Battery Spsecfication</label>\r\n                                <input\r\n                                    v-model=\"batt\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Front_camera Spsecfication</label>\r\n                                <input\r\n                                    v-model=\"front\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Back Camera_spsecfication</label>\r\n                                <input\r\n                                    type=\"text\"\r\n                                    v-model=\"back\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n\r\n                    <div class=\"detalis\">\r\n                        <div class=\"fields\">\r\n                            <div class=\"input-field\">\r\n                                <label>Screen Size</label>\r\n                                <input\r\n                                    v-model=\"screen\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Type of charge</label>\r\n                                <input\r\n                                    v-model=\"type\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Price</label>\r\n                                <input\r\n                                    v-model=\"price\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n\r\n                            <div class=\"input-field\">\r\n                                <label>Company Id</label>\r\n                                <input\r\n                                    v-model=\"comid\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Category Id</label>\r\n                                <input\r\n                                    v-model=\"catid\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>Offer Id</label>\r\n                                <input\r\n                                    v-model=\"offerid\"\r\n                                    type=\"text\"\r\n                                    placeholder=\"Enter your name\"\r\n                                />\r\n                            </div>\r\n                            <div class=\"input-field\">\r\n                                <label>imge</label>\r\n                                <input\r\n                                    type=\"file\"\r\n                                    name=\"image\"\r\n                                    accept=\"image/*\"\r\n                                    id=\"image\"\r\n                                    class=\"mx-3\"\r\n                                    @change=\"changefile\"\r\n                                />\r\n                            </div>\r\n                        </div>\r\n                        <button\r\n                            type=\"button\"\r\n                            class=\"nextbtn\"\r\n                            @click=\"Addproduct()\"\r\n                        >\r\n                            Ok\r\n                        </button>\r\n                    </div>\r\n                </div>\r\n            </form>\r\n        </div>\r\n    </body>\r\n    <section class=\"attendance\">\r\n        <div class=\"attendance-list\">\r\n            <table class=\"table\">\r\n                <thead>\r\n                    <tr>\r\n                        <th>ID</th>\r\n                        <th>Mobile_name</th>\r\n                        <th>Cpu</th>\r\n                        <th>Gpu</th>\r\n                        <th>Battery</th>\r\n                        <th>Front_camera</th>\r\n                        <th>Back_camera</th>\r\n                        <th>Screen_Size</th>\r\n                        <th>Type_of_charge</th>\r\n                        <th>Price</th>\r\n                        <th>Company_id</th>\r\n                        <th>category_id</th>\r\n                        <th>Delete</th>\r\n                        <th>Update</th>\r\n                    </tr>\r\n                </thead>\r\n                <tbody>\r\n                    <tr v-for=\"element in Products\" :key=\"element.id\">\r\n                        <td>{{ element.id }}</td>\r\n                        <td>{{ element.mobile_name }}</td>\r\n                        <td>{{ element.Cpu_spsecfication }}</td>\r\n                        <td>{{ element.Gpu_spsecfication }}</td>\r\n                        <td>{{ element.battery_spsecfication }}</td>\r\n                        <td>{{ element.Front_camera_spsecfication }}</td>\r\n                        <td>{{ element.Back_camera_spsecfication }}</td>\r\n                        <td>{{ element.Screen_Size }}</td>\r\n                        <td>{{ element.Type_of_charge }}</td>\r\n                        <td>{{ element.Price }}</td>\r\n                        <td>{{ element.Company_id }}</td>\r\n                        <td>{{ element.category_id }}</td>\r\n                        <td>\r\n                            <button\r\n                                class=\"dd\"\r\n                                @click=\"deleteproduct(element.id)\"\r\n                            >\r\n                                <i class=\"fa fa-trash\"></i>\r\n                            </button>\r\n                        </td>\r\n                        <td>\r\n                            <button class=\"dd\" @click=\"editData(element.id)\">\r\n                                <i class=\"fa fa-edit\"></i>\r\n                            </button>\r\n                        </td>\r\n                    </tr>\r\n                </tbody>\r\n            </table>\r\n        </div>\r\n    </section>\r\n    <div id=\"popup\" class=\"containeraaab\" v-show=\"isPopupVisible\">\r\n        <header>Edit Product</header>\r\n\r\n        <form action=\"#\">\r\n            <div class=\"form\">\r\n                <div class=\"detalis\">\r\n                    <span class=\"title\">Detalis </span>\r\n\r\n                    <div class=\"fields\">\r\n                        <div class=\"input-field\">\r\n                            <label>Mobile Name</label>\r\n                            <input\r\n                                v-model=\"mobilename\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>Cpu Spsecfication</label>\r\n                            <input\r\n                                v-model=\"cpu\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>Gpu Spsecfication</label>\r\n                            <input\r\n                                v-model=\"gpu\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n\r\n                        <div class=\"input-field\">\r\n                            <label>Battery Spsecfication</label>\r\n                            <input\r\n                                v-model=\"batt\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>Front_camera Spsecfication</label>\r\n                            <input\r\n                                v-model=\"front\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>Back Camera_spsecfication</label>\r\n                            <input\r\n                                type=\"text\"\r\n                                v-model=\"back\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n\r\n                <div class=\"detalis\">\r\n                    <div class=\"fields\">\r\n                        <div class=\"input-field\">\r\n                            <label>Screen Size</label>\r\n                            <input\r\n                                v-model=\"screen\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>Type of charge</label>\r\n                            <input\r\n                                v-model=\"type\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>Price</label>\r\n                            <input\r\n                                v-model=\"price\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n\r\n                        <div class=\"input-field\">\r\n                            <label>Company Id</label>\r\n                            <input\r\n                                v-model=\"comid\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>Offer Id</label>\r\n                            <input\r\n                                v-model=\"offerid\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>Category Id</label>\r\n                            <input\r\n                                v-model=\"catid\"\r\n                                type=\"text\"\r\n                                placeholder=\"Enter your name\"\r\n                            />\r\n                        </div>\r\n                        <div class=\"input-field\">\r\n                            <label>imge</label>\r\n                            <input type=\"text\" placeholder=\"Enter your name\" />\r\n                        </div>\r\n                    </div>\r\n                    <button @click=\"cancelEdit\">Cancel</button>\r\n                    <button\r\n                        type=\"button\"\r\n                        class=\"nextbtn\"\r\n                        @click=\"editproduct() + saveEdit()\"\r\n                    >\r\n                        Save\r\n                    </button>\r\n                </div>\r\n            </div>\r\n        </form>\r\n    </div>\r\n    <FooTer />\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport HeaderAdmin from \"@/components/HeaderAdmin.vue\";\r\nimport FooTer from \"@/components/footer.vue\";\r\n\r\nexport default {\r\n    name: \"AddProduct\",\r\n    components: {\r\n        HeaderAdmin,\r\n        FooTer,\r\n    },\r\n    data() {\r\n        return {\r\n            isPopupVisible: false,\r\n            comid: \"\",\r\n            img: \"\",\r\n            catid: \"\",\r\n            price: \"\",\r\n            type: \"\",\r\n            screen: \"\",\r\n            back: \"\",\r\n            front: \"\",\r\n            batt: \"\",\r\n            gpu: \"\",\r\n            offerid: \"\",\r\n            mobilename: \"\",\r\n            cpu: \"\",\r\n            Products: [],\r\n            file: null,\r\n            idpro: null,\r\n            selectedFile: null,\r\n            csv: \"\",\r\n        };\r\n    },\r\n    methods: {\r\n        changecsv(event) {\r\n            this.csv = event.target.files[0];\r\n        },\r\n        changefile() {\r\n            const input = document.getElementById(\"image\");\r\n            const file = input.files;\r\n            this.img = file;\r\n            console.log(this.img);\r\n        },\r\n\r\n        editData(qw) {\r\n            this.isPopupVisible = true;\r\n            this.idpro = qw;\r\n        },\r\n        cancelEdit() {\r\n            this.isPopupVisible = false;\r\n        },\r\n        saveEdit() {\r\n            console.log(\"save ok\");\r\n            this.isPopupVisible = false;\r\n        },\r\n\r\n        submitFile() {\r\n            const formData = new FormData();\r\n            // get the item\r\n            // append data\r\n            formData.append(\"csv_file\", this.csv);\r\n            console.log(this.csv);\r\n            const token = window.localStorage.getItem(\"token\");\r\n\r\n            axios\r\n                .post(\r\n                    \"http://127.0.0.1:8000/api/user/store/product/csv\",\r\n                    formData,\r\n\r\n                    {\r\n                        headers: {\r\n                            Authorization: `Bearer ${token}`,\r\n\r\n                            \"Content-Type\": \"multipart/form-data\",\r\n                        },\r\n                    }\r\n                )\r\n                .then((response) => {\r\n                    console.log(response);\r\n                })\r\n                .catch((error) => {\r\n                    console.log(error);\r\n                });\r\n        },\r\n\r\n        deleteproduct(id) {\r\n            axios({\r\n                method: \"post\",\r\n                url: \" http://127.0.0.1:8000/api/delete-product?id=\" + id,\r\n            })\r\n                .then((response) => {\r\n          
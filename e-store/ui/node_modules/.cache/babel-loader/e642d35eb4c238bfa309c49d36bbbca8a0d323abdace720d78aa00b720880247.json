{"ast":null,"code":"import axios from \"axios\";\nimport HeaderAdmin from \"@/components/HeaderAdmin.vue\";\nimport FooTer from \"@/components/footer.vue\";\nexport default {\n  name: \"AddProduct\",\n  components: {\n    HeaderAdmin,\n    FooTer\n  },\n  data() {\n    return {\n      isPopupVisible: false,\n      comid: \"\",\n      img: \"\",\n      catid: \"\",\n      price: \"\",\n      type: \"\",\n      screen: \"\",\n      back: \"\",\n      front: \"\",\n      batt: \"\",\n      gpu: \"\",\n      offerid: \"\",\n      mobilename: \"\",\n      cpu: \"\",\n      Products: [],\n      file: null,\n      idpro: null,\n      selectedFile: null,\n      csv: \"\"\n    };\n  },\n  methods: {\n    changecsv(event) {\n      this.csv = event.target.files[0];\n    },\n    changefile() {\n      const input = document.getElementById(\"image\");\n      const file = input.files;\n      this.img = file;\n      console.log(this.img);\n    },\n    editData(qw) {\n      this.isPopupVisible = true;\n      this.idpro = qw;\n    },\n    cancelEdit() {\n      this.isPopupVisible = false;\n    },\n    saveEdit() {\n      console.log(\"save ok\");\n      this.isPopupVisible = false;\n    },\n    submitFile() {\n      const formData = new FormData();\n      // get the item\n      // append data\n      formData.append(\"csv_file\", this.csv);\n      console.log(this.csv);\n      const token = window.localStorage.getItem(\"token\");\n      axios.post(\"http://127.0.0.1:8000/api/user/store/product/csv\", formData, {\n        headers: {\n          Authorization: `Bearer ${token}`,\n          \"Content-Type\": \"multipart/form-data\"\n        }\n      }).then(response => {\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n    },\n    deleteproduct(id) {\n      axios({\n        method: \"post\",\n        url: \" http://127.0.0.1:8000/api/delete-product?id=\" + id\n      }).then(response => {\n        console.log(response);\n        window.alert(\"OK\");\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    Addproduct() {\n      const formData = new FormData();\n      // get the item\n      const imgFile = document.querySelector(\"#image\");\n      // append data\n      formData.append(\"mobile_name\", this.mobilename);\n      formData.append(\"Cpu_spsecfication\", this.cpu);\n      formData.append(\"Gpu_spsecfication\", this.gpu);\n      formData.append(\"battery_spsecfication\", this.batt);\n      formData.append(\"Front_camera_spsecfication\", this.front);\n      formData.append(\"Back_camera_spsecfication\", this.back);\n      formData.append(\"Screen_Size\", this.screen);\n      formData.append(\"Type_of_charge\", this.type);\n      formData.append(\"Price\", this.price);\n      formData.append(\"imge\", imgFile.files[0]);\n      formData.append(\"category_id\", this.catid);\n      formData.append(\"Company_id\", this.comid);\n      formData.append(\"offer_id\", this.offerid);\n      console.log(formData);\n      axios.post(\"http://127.0.0.1:8000/api/create-product\", formData).then(response => {\n        console.log(response);\n        alert(response.data);\n      }).catch(function (error, response) {\n        console.log(error.data);\n        console.log(error.response);\n        console.log(response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    editproduct() {\n      axios({\n        method: \"post\",\n        url: \"http://127.0.0.1:8000/api/update-product?mobile_name=\" + this.mobilename + \"&Cpu_spsecfication=\" + this.cpu + \"&Gpu_spsecfication=\" + this.gpu + \"&battery_spsecfication=\" + this.batt + \"&Front_camera_spsecfication=\" + this.front + \"&Back_camera_spsecfication=\" + this.back + \"&Screen_Size=\" + this.screen + \"&Type_of_charge=\" + this.type + \"&Price=\" + this.price + \"&category_id=\" + this.catid + \"&Company_id=\" + this.comid + \"&id=\" + this.idpro,\n        data: {\n          Company_id: this.comid\n        }\n      }).then(response => {\n        console.log(response);\n        alert(response.data);\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    getproduct() {\n      axios({\n        method: \"get\",\n        url: \"http://127.0.0.1:8000/api/products/\"\n      }).then(response => {\n        console.log(response);\n        this.Products = response.data;\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    }\n  },\n  mounted() {\n    this.getproduct();\n  }\n};","map":{"version":3,"names":["axios","HeaderAdmin","FooTer","name","components","data","isPopupVisible","comid","img","catid","price","type","screen","back","front","batt","gpu","offerid","mobilename","cpu","Products","file","idpro","selectedFile","csv","methods","changecsv","event","target","files","changefile","input","document","getElementById","console","log","editData","qw","cancelEdit","saveEdit","submitFile","formData","FormData","append","token","window","localStorage","getItem","post","headers","Authorization","then","response","catch","error","deleteproduct","id","method","url","alert","Addproduct","imgFile","querySelector","editproduct","Company_id","getproduct","mounted"],"sources":["C:\\Users\\ASUSD\\Documents\\GitHub\\e-store-website\\e-store\\ui\\src\\views\\AddProduct.vue"],"sourcesContent":["<template>\n    <body>\n        <HeaderAdmin />\n        <div class=\"oo\">\n            <p class=\"order\">Mangment Product</p>\n            <div class=\"add-product\">\n                <button class=\"s1\" @click=\"submitFile\">Import</button>\n\n                <input\n                    type=\"file\"\n                    @change=\"changecsv\"\n                    accept=\"csv/*\"\n                    ref=\"file\"\n                    id=\"csv\"\n                    name=\"csv\"\n                />\n            </div>\n        </div>\n        <div class=\"container\">\n            <header>Add Product</header>\n\n            <form action=\"#\">\n                <div class=\"form\">\n                    <div class=\"detalis\">\n                        <span class=\"title\">Detalis </span>\n\n                        <div class=\"fields\">\n                            <div class=\"input-field\">\n                                <label>Mobile Name</label>\n                                <input\n                                    v-model=\"mobilename\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Cpu Spsecfication</label>\n                                <input\n                                    v-model=\"cpu\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Gpu Spsecfication</label>\n                                <input\n                                    v-model=\"gpu\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n\n                            <div class=\"input-field\">\n                                <label>Battery Spsecfication</label>\n                                <input\n                                    v-model=\"batt\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Front_camera Spsecfication</label>\n                                <input\n                                    v-model=\"front\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Back Camera_spsecfication</label>\n                                <input\n                                    type=\"text\"\n                                    v-model=\"back\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                        </div>\n                    </div>\n\n                    <div class=\"detalis\">\n                        <div class=\"fields\">\n                            <div class=\"input-field\">\n                                <label>Screen Size</label>\n                                <input\n                                    v-model=\"screen\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Type of charge</label>\n                                <input\n                                    v-model=\"type\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Price</label>\n                                <input\n                                    v-model=\"price\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n\n                            <div class=\"input-field\">\n                                <label>Company Id</label>\n                                <input\n                                    v-model=\"comid\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Category Id</label>\n                                <input\n                                    v-model=\"catid\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Offer Id</label>\n                                <input\n                                    v-model=\"offerid\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>imge</label>\n                                <input\n                                    type=\"file\"\n                                    name=\"image\"\n                                    accept=\"image/*\"\n                                    id=\"image\"\n                                    class=\"mx-3\"\n                                    @change=\"changefile\"\n                                />\n                            </div>\n                        </div>\n                        <button\n                            type=\"button\"\n                            class=\"nextbtn\"\n                            @click=\"Addproduct()\"\n                        >\n                            Ok\n                        </button>\n                    </div>\n                </div>\n            </form>\n        </div>\n    </body>\n    <section class=\"attendance\">\n        <div class=\"attendance-list\">\n            <table class=\"table\">\n                <thead>\n                    <tr>\n                        <th>ID</th>\n                        <th>Mobile_name</th>\n                        <th>Cpu</th>\n                        <th>Gpu</th>\n                        <th>Battery</th>\n                        <th>Front_camera</th>\n                        <th>Back_camera</th>\n                        <th>Screen_Size</th>\n                        <th>Type_of_charge</th>\n                        <th>Price</th>\n                        <th>Company_id</th>\n                        <th>category_id</th>\n                        <th>Delete</th>\n                        <th>Update</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    <tr v-for=\"element in Products\" :key=\"element.id\">\n                        <td>{{ element.id }}</td>\n                        <td>{{ element.mobile_name }}</td>\n                        <td>{{ element.Cpu_spsecfication }}</td>\n                        <td>{{ element.Gpu_spsecfication }}</td>\n                        <td>{{ element.battery_spsecfication }}</td>\n                        <td>{{ element.Front_camera_spsecfication }}</td>\n                        <td>{{ element.Back_camera_spsecfication }}</td>\n                        <td>{{ element.Screen_Size }}</td>\n                        <td>{{ element.Type_of_charge }}</td>\n                        <td>{{ element.Price }}</td>\n                        <td>{{ element.Company_id }}</td>\n                        <td>{{ element.category_id }}</td>\n                        <td>\n                            <button\n                                class=\"dd\"\n                                @click=\"deleteproduct(element.id)\"\n                            >\n                                <i class=\"fa fa-trash\"></i>\n                            </button>\n                        </td>\n                        <td>\n                            <button class=\"dd\" @click=\"editData(element.id)\">\n                                <i class=\"fa fa-edit\"></i>\n                            </button>\n                        </td>\n                    </tr>\n                </tbody>\n            </table>\n        </div>\n    </section>\n    <div id=\"popup\" class=\"containeraaab\" v-show=\"isPopupVisible\">\n        <header>Edit Product</header>\n\n        <form action=\"#\">\n            <div class=\"form\">\n                <div class=\"detalis\">\n                    <span class=\"title\">Detalis </span>\n\n                    <div class=\"fields\">\n                        <div class=\"input-field\">\n                            <label>Mobile Name</label>\n                            <input\n                                v-model=\"mobilename\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>Cpu Spsecfication</label>\n                            <input\n                                v-model=\"cpu\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>Gpu Spsecfication</label>\n                            <input\n                                v-model=\"gpu\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n\n                        <div class=\"input-field\">\n                            <label>Battery Spsecfication</label>\n                            <input\n                                v-model=\"batt\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>Front_camera Spsecfication</label>\n                            <input\n                                v-model=\"front\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>Back Camera_spsecfication</label>\n                            <input\n                                type=\"text\"\n                                v-model=\"back\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                    </div>\n                </div>\n\n                <div class=\"detalis\">\n                    <div class=\"fields\">\n                        <div class=\"input-field\">\n                            <label>Screen Size</label>\n                            <input\n                                v-model=\"screen\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>Type of charge</label>\n                            <input\n                                v-model=\"type\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>Price</label>\n                            <input\n                                v-model=\"price\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n\n                        <div class=\"input-field\">\n                            <label>Company Id</label>\n                            <input\n                                v-model=\"comid\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>Offer Id</label>\n                            <input\n                                v-model=\"offerid\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>Category Id</label>\n                            <input\n                                v-model=\"catid\"\n                                type=\"text\"\n                                placeholder=\"Enter your name\"\n                            />\n                        </div>\n                        <div class=\"input-field\">\n                            <label>imge</label>\n                            <input type=\"text\" placeholder=\"Enter your name\" />\n                        </div>\n                    </div>\n                    <button @click=\"cancelEdit\">Cancel</button>\n                    <button\n                        type=\"button\"\n                        class=\"nextbtn\"\n                        @click=\"editproduct() + saveEdit()\"\n                    >\n                        Save\n                    </button>\n                </div>\n            </div>\n        </form>\n    </div>\n    <FooTer />\n</template>\n<script>\nimport axios from \"axios\";\nimport HeaderAdmin from \"@/components/HeaderAdmin.vue\";\nimport FooTer from \"@/components/footer.vue\";\n\nexport default {\n    name: \"AddProduct\",\n    components: {\n        HeaderAdmin,\n        FooTer,\n    },\n    data() {\n        return {\n            isPopupVisible: false,\n            comid: \"\",\n            img: \"\",\n            catid: \"\",\n            price: \"\",\n            type: \"\",\n            screen: \"\",\n            back: \"\",\n            front: \"\",\n            batt: \"\",\n            gpu: \"\",\n            offerid: \"\",\n            mobilename: \"\",\n            cpu: \"\",\n            Products: [],\n            file: null,\n            idpro: null,\n            selectedFile: null,\n            csv: \"\",\n        };\n    },\n    methods: {\n        changecsv(event) {\n            this.csv = event.target.files[0];\n        },\n        changefile() {\n            const input = document.getElementById(\"image\");\n            const file = input.files;\n            this.img = file;\n            console.log(this.img);\n        },\n\n        editData(qw) {\n            this.isPopupVisible = true;\n            this.idpro = qw;\n        },\n        cancelEdit() {\n            this.isPopupVisible = false;\n        },\n        saveEdit() {\n            console.log(\"save ok\");\n            this.isPopupVisible = false;\n        },\n\n        submitFile() {\n            const formData = new FormData();\n            // get the item\n            // append data\n            formData.append(\"csv_file\", this.csv);\n            console.log(this.csv);\n            const token = window.localStorage.getItem(\"token\");\n\n            axios\n                .post(\n                    \"http://127.0.0.1:8000/api/user/store/product/csv\",\n                    formData,\n\n                    {\n                        headers: {\n                            Authorization: `Bearer ${token}`,\n\n                            \"Content-Type\": \"multipart/form-data\",\n                        },\n                    }\n                )\n                .then((response) => {\n                    console.log(response);\n                })\n                .catch((error) => {\n                    console.log(error);\n                });\n        },\n\n        deleteproduct(id) {\n            axios({\n                method: \"post\",\n                url: \" http://127.0.0.1:8000/api/delete-product?id=\" + id,\n            })\n                .then((response) => {\n                    console.log(response);\n                    window.alert(\"OK\");\n                })\n                .catch(function (error) {\n                    console.log(error.data);\n                    console.log(error.response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n\n        Addproduct() {\n            const formData = new FormData();\n            // get the item\n            const imgFile = document.querySelector(\"#image\");\n            // append data\n            formData.append(\"mobile_name\", this.mobilename);\n            formData.append(\"Cpu_spsecfication\", this.cpu);\n            formData.append(\"Gpu_spsecfication\", this.gpu);\n            formData.append(\"battery_spsecfication\", this.batt);\n            formData.append(\"Front_camera_spsecfication\", this.front);\n            formData.append(\"Back_camera_spsecfication\", this.back);\n            formData.append(\"Screen_Size\", this.screen);\n            formData.append(\"Type_of_charge\", this.type);\n            formData.append(\"Price\", this.price);\n            formData.append(\"imge\", imgFile.files[0]);\n            formData.append(\"category_id\", this.catid);\n            formData.append(\"Company_id\", this.comid);\n            formData.append(\"offer_id\", this.offerid);\n            console.log(formData);\n\n            axios\n                .post(\n                    \"http://127.0.0.1:8000/api/create-product\",\n\n                    formData\n                )\n                .then((response) => {\n                    console.log(response);\n                    alert(response.data);\n                })\n                .catch(function (error, response) {\n                    console.log(error.data);\n                    console.log(error.response);\n                    console.log(response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n        editproduct() {\n            axios({\n                method: \"post\",\n                url:\n                    \"http://127.0.0.1:8000/api/update-product?mobile_name=\" +\n                    this.mobilename +\n                    \"&Cpu_spsecfication=\" +\n                    this.cpu +\n                    \"&Gpu_spsecfication=\" +\n                    this.gpu +\n                    \"&battery_spsecfication=\" +\n                    this.batt +\n                    \"&Front_camera_spsecfication=\" +\n                    this.front +\n                    \"&Back_camera_spsecfication=\" +\n                    this.back +\n                    \"&Screen_Size=\" +\n                    this.screen +\n                    \"&Type_of_charge=\" +\n                    this.type +\n                    \"&Price=\" +\n                    this.price +\n                    \"&category_id=\" +\n                    this.catid +\n                    \"&Company_id=\" +\n                    this.comid +\n                    \"&id=\" +\n                    this.idpro,\n                data: {\n                    Company_id: this.comid,\n                },\n            })\n                .then((response) => {\n                    console.log(response);\n                    alert(response.data);\n                })\n                .catch(function (error) {\n                    console.log(error.data);\n                    console.log(error.response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n        getproduct() {\n            axios({\n                method: \"get\",\n                url: \"http://127.0.0.1:8000/api/products/\",\n            })\n                .then((response) => {\n                    console.log(response);\n                    this.Products = response.data;\n                })\n                .catch(function (error) {\n                    console.log(error.data);\n                    console.log(error.response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n    },\n    mounted() {\n        this.getproduct();\n    },\n};\n</script>\n<style scoped>\n* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n    font-family: \"poppins\", \"sans-serif\";\n}\n.table {\n    border-collapse: collapse;\n    table-layout: fixed;\n    font-size: 15px;\n    min-width: 100%;\n    overflow: hidden;\n    border-radius: 4px;\n}\n\n.table thead tr {\n    background: #000000;\n    text-align: left;\n    font-weight: bold;\n    color: white;\n}\n\n.table th {\n    padding: 12px 15px;\n    text-align: center;\n    word-break: break-all;\n    color: white;\n}\ntr td a {\n    color
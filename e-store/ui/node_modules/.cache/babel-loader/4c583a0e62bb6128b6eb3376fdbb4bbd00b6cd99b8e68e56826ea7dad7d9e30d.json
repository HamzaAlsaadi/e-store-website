{"ast":null,"code":"import HeaderAdmin from \"@/components/HeaderAdmin.vue\";\nimport FooTer from \"@/components/footer.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"CompanyAdmin\",\n  components: {\n    HeaderAdmin,\n    FooTer\n  },\n  data() {\n    return {\n      isPopupVisible: false,\n      namecompany: \"\",\n      address: \"\",\n      companies: [],\n      idpro: null,\n      img: \"\"\n    };\n  },\n  methods: {\n    changefile() {\n      const input = document.getElementById(\"image\");\n      const file = input.files;\n      this.img = file;\n      console.log(this.img);\n    },\n    editData(qw) {\n      this.isPopupVisible = true;\n      this.idpro = qw;\n    },\n    cancelEdit() {\n      this.isPopupVisible = false;\n    },\n    saveEdit() {\n      console.log(\"save ok\");\n      this.isPopupVisible = false;\n    },\n    Addcompany() {\n      const formData = new FormData();\n      // get the item\n      const imgFile = document.querySelector(\"#image\");\n      // append data\n      formData.append(\"company_name\", this.namecompany);\n      formData.append(\"company_address\", this.address);\n      formData.append(\"image\", imgFile.files[0]);\n      console.log(formData);\n      axios.post(\"http://127.0.0.1:8000/api/create-Company\", formData).then(response => {\n        console.log(response);\n        alert(response.data);\n      }).catch(function (error, response) {\n        console.log(error.data);\n        console.log(error.response);\n        console.log(response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    editcompany() {\n      axios({\n        method: \"post\",\n        url: \"http://127.0.0.1:8000/api/update-Company?company_name=\" + this.namecompany + \"&company_address=\" + this.address + \"&id=\" + this.idpro\n      }).then(response => {\n        console.log(response);\n        alert(response.data);\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    getcompany() {\n      axios({\n        method: \"get\",\n        url: \"http://127.0.0.1:8000/api/Company/\"\n      }).then(response => {\n        console.log(response);\n        this.companies = response.data;\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    },\n    deletecompany(id) {\n      axios({\n        method: \"post\",\n        url: \"http://127.0.0.1:8000/api/delete-Company?id=\" + id\n      }).then(response => {\n        console.log(response);\n        window.location.reload();\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    }\n  },\n  mounted() {\n    this.getcompany();\n  }\n};","map":{"version":3,"names":["HeaderAdmin","FooTer","axios","name","components","data","isPopupVisible","namecompany","address","companies","idpro","img","methods","changefile","input","document","getElementById","file","files","console","log","editData","qw","cancelEdit","saveEdit","Addcompany","formData","FormData","imgFile","querySelector","append","post","then","response","alert","catch","error","window","editcompany","method","url","getcompany","deletecompany","id","location","reload","mounted"],"sources":["C:\\Users\\ASUSD\\Documents\\GitHub\\e-store-website\\e-store\\ui\\src\\views\\CompanyAdmin.vue"],"sourcesContent":["<template>\n    <body>\n        <HeaderAdmin />\n        <div class=\"container\">\n            <header>Add Company</header>\n\n            <form action=\"#\">\n                <div class=\"form\">\n                    <div class=\"detalis\">\n                        <span class=\"title\">Detalis </span>\n\n                        <div class=\"fields\">\n                            <div class=\"input-field\">\n                                <label> Name Company </label>\n                                <input\n                                    v-model=\"namecompany\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Address Company </label>\n                                <input\n                                    v-model=\"address\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n\n                            <div class=\"input-field\">\n                                <label>imge</label>\n                                <input\n                                    type=\"file\"\n                                    name=\"image\"\n                                    accept=\"image/*\"\n                                    id=\"image\"\n                                    class=\"mx-3\"\n                                    @change=\"changefile\"\n                                />\n                            </div>\n                        </div>\n                        <button\n                            type=\"button\"\n                            class=\"nextbtn\"\n                            @click=\"Addcompany()\"\n                        >\n                            Ok\n                        </button>\n                    </div>\n                </div>\n            </form>\n        </div>\n        <section class=\"attendance\">\n            <div class=\"attendance-list\">\n                <table class=\"table\">\n                    <thead>\n                        <tr>\n                            <th>ID</th>\n                            <th>Name</th>\n                            <th>address</th>\n                            <th>created at</th>\n                            <th>updated at</th>\n                            <th>Delete</th>\n                            <th>Edit</th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        <tr v-for=\"element in companies\" :key=\"element.id\">\n                            <td>{{ element.id }}</td>\n                            <td>{{ element.company_name }}</td>\n                            <td>{{ element.company_address }}</td>\n                            <td>{{ element.created_at }}</td>\n                            <td>{{ element.updated_at }}</td>\n\n                            <td>\n                                <button\n                                    class=\"dd\"\n                                    @click=\"deletecompany(element.id)\"\n                                >\n                                    <i class=\"fa fa-trash\"></i>\n                                </button>\n                            </td>\n                            <td>\n                                <button\n                                    class=\"dd\"\n                                    @click=\"editData(element.id)\"\n                                >\n                                    <i class=\"fa fa-edit\"></i>\n                                </button>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n        </section>\n        <div id=\"popup\" class=\"containeraaab\" v-show=\"isPopupVisible\">\n            <header>Edit Company</header>\n\n            <form action=\"#\">\n                <div class=\"form\">\n                    <div class=\"detalis\">\n                        <span class=\"title\">Detalis </span>\n\n                        <div class=\"fields\">\n                            <div class=\"input-field\">\n                                <label> Name Company </label>\n                                <input\n                                    v-model=\"namecompany\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                            <div class=\"input-field\">\n                                <label>Address Company </label>\n                                <input\n                                    v-model=\"address\"\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n\n                            <div class=\"input-field\">\n                                <label>imge</label>\n                                <input\n                                    type=\"text\"\n                                    placeholder=\"Enter your name\"\n                                />\n                            </div>\n                        </div>\n                        <button @click=\"cancelEdit\">Cancel</button>\n                        <button\n                            type=\"button\"\n                            class=\"nextbtn\"\n                            @click=\"editcompany() + saveEdit()\"\n                        >\n                            Save\n                        </button>\n                    </div>\n                </div>\n            </form>\n        </div>\n\n        <FooTer />\n    </body>\n</template>\n<script>\nimport HeaderAdmin from \"@/components/HeaderAdmin.vue\";\nimport FooTer from \"@/components/footer.vue\";\nimport axios from \"axios\";\n\nexport default {\n    name: \"CompanyAdmin\",\n    components: {\n        HeaderAdmin,\n        FooTer,\n    },\n    data() {\n        return {\n            isPopupVisible: false,\n\n            namecompany: \"\",\n            address: \"\",\n            companies: [],\n            idpro: null,\n            img: \"\",\n        };\n    },\n    methods: {\n        changefile() {\n            const input = document.getElementById(\"image\");\n            const file = input.files;\n            this.img = file;\n            console.log(this.img);\n        },\n        editData(qw) {\n            this.isPopupVisible = true;\n            this.idpro = qw;\n        },\n        cancelEdit() {\n            this.isPopupVisible = false;\n        },\n        saveEdit() {\n            console.log(\"save ok\");\n            this.isPopupVisible = false;\n        },\n\n        Addcompany() {\n            const formData = new FormData();\n            // get the item\n            const imgFile = document.querySelector(\"#image\");\n            // append data\n            formData.append(\"company_name\", this.namecompany);\n            formData.append(\"company_address\", this.address);\n            formData.append(\"image\", imgFile.files[0]);\n            console.log(formData);\n\n            axios\n                .post(\n                    \"http://127.0.0.1:8000/api/create-Company\",\n\n                    formData\n                )\n                .then((response) => {\n                    console.log(response);\n                    alert(response.data);\n                })\n                .catch(function (error, response) {\n                    console.log(error.data);\n                    console.log(error.response);\n                    console.log(response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n        editcompany() {\n            axios({\n                method: \"post\",\n                url:\n                    \"http://127.0.0.1:8000/api/update-Company?company_name=\" +\n                    this.namecompany +\n                    \"&company_address=\" +\n                    this.address +\n                    \"&id=\" +\n                    this.idpro,\n            })\n                .then((response) => {\n                    console.log(response);\n                    alert(response.data);\n                })\n                .catch(function (error) {\n                    console.log(error.data);\n                    console.log(error.response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n        getcompany() {\n            axios({\n                method: \"get\",\n                url: \"http://127.0.0.1:8000/api/Company/\",\n            })\n                .then((response) => {\n                    console.log(response);\n                    this.companies = response.data;\n                })\n                .catch(function (error) {\n                    console.log(error.data);\n                    console.log(error.response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n        deletecompany(id) {\n            axios({\n                method: \"post\",\n                url: \"http://127.0.0.1:8000/api/delete-Company?id=\" + id,\n            })\n                .then((response) => {\n                    console.log(response);\n                    window.location.reload();\n                })\n                .catch(function (error) {\n                    console.log(error.data);\n                    console.log(error.response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n    },\n    mounted() {\n        this.getcompany();\n    },\n};\n</script>\n<style scoped>\n* {\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n    font-family: \"poppins\", \"sans-serif\";\n}\n.table {\n    border-collapse: collapse;\n    table-layout: fixed;\n    font
order-radius: 50%;\r\n    height: 100%;\r\n    background: #fff;\r\n    transition: all 0.6s ease;\r\n}\r\n\r\n.wrapper.active .form.login {\r\n    bottom: -15%;\r\n    border-radius: 35%;\r\n    box-shadow: 0 -5px 10px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.form header {\r\n    font-size: 30px;\r\n    text-align: center;\r\n    color: #fff;\r\n    font-weight: 600;\r\n    cursor: pointer;\r\n}\r\n\r\n\r\n</style>\r\n"],"mappings":";;EACUA,IAAI,EAAC;AAAI;;oBACXC,UAoBO,C;;uBArBXC,mBAAA,CA4EO,QA5EPC,UA4EO,EAAAC,UAAA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
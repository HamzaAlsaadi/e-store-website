{"ast":null,"code":"import HeaderUserAccount from \"@/components/HeaderUserAccount.vue\";\nimport FooTer from \"@/components/footer.vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"ContactUs\",\n  components: {\n    HeaderUserAccount,\n    FooTer\n  },\n  data() {\n    return {\n      email: \"\",\n      content: \"\"\n    };\n  },\n  methods: {\n    sendmessage() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        method: \"post\",\n        url: \"http://127.0.0.1:8000/api/send-message\",\n        data: {\n          email: this.email,\n          content: this.content\n        },\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      }).then(response => {\n        console.log(response);\n      }).catch(function (error) {\n        console.log(error.data);\n        console.log(error.response);\n      }).catch(function () {\n        window.alert(\"hi\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["HeaderUserAccount","FooTer","axios","name","components","data","email","content","methods","sendmessage","token","window","localStorage","getItem","method","url","headers","Authorization","then","response","console","log","catch","error","alert"],"sources":["C:\\Users\\ASUSD\\Documents\\GitHub\\e-store-website\\e-store\\ui\\src\\views\\ContactUs.vue"],"sourcesContent":["<template>\n    <link\n        rel=\"stylesheet\"\n        href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css\"\n    />\n\n    <HeaderUserAccount />\n    <div class=\"contact-form\">\n        <h1>Contact Us</h1>\n        <div class=\"container\">\n            <div class=\"main\">\n                <div class=\"content\">\n                    <h2>Contact Us</h2>\n                    <label for=\"\">Email :</label>\n                    <input\n                        type=\"email\"\n                        name=\"name\"\n                        placeholder=\"Enter Your Email\"\n                        v-model=\"email\"\n                    />\n                    <label for=\"\">Message :</label>\n                    <textarea\n                        name=\"message\"\n                        placeholder=\"Your Message\"\n                        v-model=\"content\"\n                    ></textarea>\n                    <button class=\"btn\" @click=\"sendmessage()\">\n                        Send <i class=\"fas fa-paper-plane\"></i>\n                    </button>\n                </div>\n                <div class=\"form-img\"></div>\n            </div>\n        </div>\n    </div>\n    <FooTer />\n</template>\n<script>\nimport HeaderUserAccount from \"@/components/HeaderUserAccount.vue\";\nimport FooTer from \"@/components/footer.vue\";\nimport axios from \"axios\";\n\nexport default {\n    name: \"ContactUs\",\n    components: { HeaderUserAccount, FooTer },\n    data() {\n        return {\n            email: \"\",\n            content: \"\",\n        };\n    },\n    methods: {\n        sendmessage() {\n            const token = window.localStorage.getItem(\"token\");\n\n            axios({\n                method: \"post\",\n                url: \"http://127.0.0.1:8000/api/send-message\",\n                data: {\n                    email: this.email,\n                    content: this.content,\n                },\n                headers: { Authorization: `Bearer ${token}` },\n            })\n                .then((response) => {\n                    console.log(response);\n                })\n                .catch(function (error) {\n                    console.log(error.data);\n                    console.log(error.response);\n                })\n                .catch(function () {\n                    window.alert(\"hi\");\n                });\n        },\n    },\n};\n</script>\n<style scoped>\n@import url(\"https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&display=swap\");\n* {\n    padding: 0;\n    margin: 0;\n    box-sizing: border-box;\n    font-family: \"poppins\", sans-serif;\n}\n.contact-form {\n    position: relative;\n    min-height: 100vh;\n    z-index: 0;\n    background: #000000;\n    padding: 30px;\n    justify-content: center;\n    display: grid;\n    grid-template-rows: 1fr auto 1fr;\n    align-items: center;\n}\n.container {\n    max-width: 800px;\n    margin-top: 0 auto;\n}\n.contact-form h1 {\n    text-align: center;\n    font-size: 2.5rem;\n    font-weight: 400;\n    color: #fff;\n    font-family: \"poppins\";\n}\n.contact-form h2 {\n    line-height: 40px;\n    margin-bottom: 5px;\n    font-size: 30px;\n    font-weight: 500;\n    color: #000000;\n    text-align: center;\n}\n.contact-form .main {\n    position: relative;\n    display: flex;\n    margin: 30px 0;\n}\n.content {\n    flex-basis: 50%;\n    padding: 3em 3em;\n    background: #fff;\n    box-shadow: 2px 9px 49px -17px rgba(156, 39, 148, 0.1);\n    border-top-left-radius: 8px;\n    border-bottom-left-radius: 8px;\n}\n.form-img {\n    flex-basis: 50%;\n    background: #f0f4f8;\n    background-size: cover;\n    padding: 40px;\n    border-top-right-radius: 8px;\n    border-bottom-right-radius: 8px;\n    align-items: center;\n    display: grid;\n}\n.form-img img {\n    max-width: 100%;\n}\n.btn,\nbutton,\ninput {\n    border-radius: 35px;\n}\n.btn:hover,\nbutton:hover {\n    color: #3e3d3e;\n    transition: 0.5s ease;\n}\n.contact-form form {\n    margin: 30px 0;\n}\n.contact-form input,\ntextarea {\n    outline: none;\n    margin-bottom: 15px;\n    font-stretch: 16px;\n    color: #000000;\n    padding: 14px 20px;\n    width: 100%;\n    display: inline-block;\n    box-sizing: border-box;\n    border: none;\n    background: #fcfcfc;\n    transition: 0.3s ease;\n}\n.contact-form input:focus {\n    background: transparent;\n    border: 1px solid #000000;\n}\n.contact-form button {\n    font-size: 18px;\n    color: #fff;\n    width: 100%;\n    background: #000000;\n    font-weight: 600;\n    transition: 0.3s ease;\n    padding: 14px 15px;\n    border: 1px solid #ededed;\n}\n@media (max-width: 736px) {\n    .contact-form .main {\n        flex-direction: column;\n    }\n    .contact-form form {\n        margin-top: 30px;\n        margin-bottom: 10px;\n    }\n    .form-img {\n        border-radius: 0;\n        border-bottom-left-radius: 8px;\n        border-bottom-right-radius: 8px;\n        order: 2;\n    }\n    .content {\n        order: 1;\n        border-top-left-radius: 8px;\n        border-top-right-radius: 8px;\n    }\n}\n</style>\n"],"mappings":"AAqCA,OAAOA,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,MAAK,MAAO,yBAAyB;AAC5C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAAEJ,iBAAiB;IAAEC;EAAO,CAAC;EACzCI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACb,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,WAAWA,CAAA,EAAG;MACV,MAAMC,KAAI,GAAIC,MAAM,CAACC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAElDX,KAAK,CAAC;QACFY,MAAM,EAAE,MAAM;QACdC,GAAG,EAAE,wCAAwC;QAC7CV,IAAI,EAAE;UACFC,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,OAAO,EAAE,IAAI,CAACA;QAClB,CAAC;QACDS,OAAO,EAAE;UAAEC,aAAa,EAAG,UAASP,KAAM;QAAE;MAChD,CAAC,EACIQ,IAAI,CAAEC,QAAQ,IAAK;QAChBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC;MACzB,CAAC,EACAG,KAAK,CAAC,UAAUC,KAAK,EAAE;QACpBH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAClB,IAAI,CAAC;QACvBe,OAAO,CAACC,GAAG,CAACE,KAAK,CAACJ,QAAQ,CAAC;MAC/B,CAAC,EACAG,KAAK,CAAC,YAAY;QACfX,MAAM,CAACa,KAAK,CAAC,IAAI,CAAC;MACtB,CAAC,CAAC;IACV;EACJ;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}